{
  "tsserver.enableJavascript": true,
  "diagnostic.enable": true,
  "diagnostic.warningSign": "âš ",
  "diagnostic.errorSign": "Ã—",
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "imselect.enableStatusItem": false,
  "imselect.enableFloating": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "python", 
    "rust", 
    "css",
    "markdown",
    "javascript",
    "typescript",
    "javascript.jsx",
    "html",
    "json",
    "jsonc"
  ],
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.floatActions": false,
  "coc.preferences.promptInput": false,
  "prettier.requireConfig": true,
  "prettier.statusItemText": "ðŸ’…  ",
  "colors.filetypes": [
    "*"
  ],
  "highlight.colors.enable": false,
  "rust-analyzer.updates.checkOnStartup": false,
  "rust-analyzer.updates.prompt": false,
  "rust-analyzer.inlayHints.chainingHints.enable": false,
  "rust-analyzer.inlayHints.closingBraceHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "coc.preferences.extensionUpdateCheck": "never",
  "sumneko-lua.enableNvimLuaDev": true,
  "python.pythonPath": "python",
  "languageserver": {
    "sourcekit-lsp": {
      "filetypes": [
        "swift",
        "c",
        "cpp",
        "objc",
        "objcpp"
      ],
      "command": "sourcekit-lsp"
    },
    "ccls": {
      "command": "ccls",
      "filetypes": [
      ],
      "rootPatterns": [
        ".ccls-root",
        "compile_commands.json"
      ],
      "initializationOptions": {
        "cache": {
          "directory": ".ccls-cache"
        }
      }
    }
  },
  "Lua.telemetry.enable": true
